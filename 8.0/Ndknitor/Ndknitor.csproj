<Project Sdk="Microsoft.NET.Sdk.Web">

  <PropertyGroup>
    <TargetFramework>net8.0</TargetFramework>
    <ImplicitUsings>enable</ImplicitUsings>
    <IsPackable>true</IsPackable>
    <OutputType>Library</OutputType>
    <PackageReadmeFile>NREADME.md</PackageReadmeFile>
    <Nullable>disable</Nullable>
    <PackageId>Ndknitor</PackageId>
    <Version>8.0.1</Version>
    <Authors>Ngo Dinh Khoi Nguyen</Authors>
    <Company>Ndknitor</Company>
    <ProjectUrl>https://github.com/ndknitor/NdknitorDotNet</ProjectUrl>
    <RepositoryUrl>https://github.com/ndknitor/NdknitorDotNet</RepositoryUrl>
    <RepositoryType>git</RepositoryType>
    <PackageLicenseFile>LICENSE.md</PackageLicenseFile>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Microsoft.EntityFrameworkCore.Relational" Version="8.0.0" />
    <PackageReference Include="Microsoft.Extensions.Caching.StackExchangeRedis" Version="8.0.0" />
    <PackageReference Include="Newtonsoft.Json" Version="13.0.3" />
    <PackageReference Include="Newtonsoft.Json.Bson" Version="1.0.2" />
    <PackageReference Include="SCrypt" Version="2.0.0.2" />
    <PackageReference Include="SixLabors.ImageSharp" Version="3.0.2" />
    <PackageReference Include="Z.EntityFramework.Plus.EFCore" Version="8.101.0" />
    <None Include="NREADME.md" Pack="true" PackagePath="/" />
    <None Include="../../LICENSE.md">
      <Pack>true</Pack>
      <PackagePath>/</PackagePath>
    </None>
  </ItemGroup>

</Project>
